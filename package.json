{
  "name": "@foo-dog/indent-transformer",
  "version": "0.1.0",
  "description": "Transform stream to take lines and return indentation indicators",
  "main": "dist/index.js",
  "source": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "parcel build",
    "test": "mocha test/*.test.*",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "stream",
    "transform",
    "indentation",
    "text",
    "indent",
    "whitespace"
  ],
  "author": "Adam Koch (aakoch)",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-equal-for-long-text": "^0.2.1",
    "concat-stream": "^2.0.0",
    "filecompare": "^1.0.4",
    "mocha": "^9.1.2",
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "@jaredpalmer/wrapline": "^2.0.2",
    "debug": "^4.3.2"
  },
  "type": "module",
  "engines": {
    "node": ">=8"
  },
  "targets": {
    "main": {
      "optimize": true,
      "isLibrary": true
    }
  }
}
